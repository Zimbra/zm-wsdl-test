      <output message="tns:createDomainResponse"/>
    </operation>
    <operation name="createServerRequest">
      <input message="tns:createServerRequest"/>
      <output message="tns:createServerResponse"/>
    </operation>
    <operation name="deleteDomainRequest">
      <input message="tns:deleteDomainRequest"/>
      <output message="tns:deleteDomainResponse"/>
    </operation>
    <operation name="deleteServerRequest">
      <input message="tns:deleteServerRequest"/>
